@page "/"
@namespace invMed.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>invMed</title>
    <base href="~/" />
    <link href="css/site.css" rel="stylesheet" />
    <link href="invMed.styles.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&display=swap" rel="stylesheet" />
    <link href="_content/MudBlazor/MudBlazor.min.css" rel="stylesheet" />
</head>
<body>
    <component type="typeof(App)" render-mode="ServerPrerendered" />

    <div id="blazor-error-ui">
        <environment include="Staging,Production">
            Wystapił błąd. Przeładuj aplikację.
        </environment>
        <environment include="Development">
            Wystąpił wyjątek. Sprawdź narzędzia deweloperskie w przeglądarce.
        </environment>
        <a href="" class="reload">Przeładuj</a>
        <a class="dismiss">🗙</a>
    </div>

<script src="_framework/blazor.server.js"></script>
<script src="_content/MudBlazor/MudBlazor.min.js"></script>
<script src="~/js/pdfDownload.js"></script>
</body>
</html>
